{
  "version": 2,
  "builds": [
    {
      "src": "server/vercel.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/vercel.js"
    },
    {
      "src": "/static/(.*)",
      "dest": "build/static/$1"
    },
    {
      "src": "/favicon.ico",
      "dest": "build/favicon.ico"
    },
    {
      "src": "/manifest.json",
      "dest": "build/manifest.json"
    },
    {
      "src": "/logo192.png",
      "dest": "build/logo192.png"
    },
    {
      "src": "/logo512.png",
      "dest": "build/logo512.png"
    },
    {
      "src": "/(.*)",
      "dest": "build/index.html"
    }
  ]
}
